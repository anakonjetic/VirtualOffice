@model List<VirtualOffice.Models.Employee>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Position</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.FirstName</td>
                <td>@employee.LastName</td>
                <td>@employee.DateOfBirth</td>
            </tr>
        }
    </tbody>
</table>
